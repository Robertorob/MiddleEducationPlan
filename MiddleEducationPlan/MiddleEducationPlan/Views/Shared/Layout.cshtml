<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AzureTest</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="~/lib/chosen/chosen.css">

    <link rel="stylesheet" href="~/css/loader.css" />
    <link rel="stylesheet" href="~/css/select.css" />
    <link rel="stylesheet" href="~/css/dialog.css" />

    <link rel="stylesheet" href="~/css/important.css" />

    <script src="~/lib/jquery/jquery.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.19.2/dist/jquery.validate.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.19.2/dist/additional-methods.js"></script>

    <script src="~/lib/chosen/chosen.jquery.js"></script>

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" href="@Url.Action("Index", "Home")">Middle education plan</a>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("Index", "ProjectView")">Projects</a>
                        </li>
                        @*<li class="nav-item">
                            <a class="nav-link" href="@Url.Action("Index", "TaskView")">Tasks</a>
                        </li>*@
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container" style="width:50%;margin-left:20%">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - Middle Education Plan
        </div>
    </footer>


    <div style="display:none" id="dialog" title="">
        <p id="dialogBody"></p>
    </div>

    <div style="display:none" id="deleteDialog" title="">
        <p id="deleteDialogBody"></p>
    </div>

    <div style="display:none" class="loader"></div>


    @RenderSection("Scripts", required: false)
</body>
</html>